<odoo>
    <record id="sequence_account_payment_inbound_send" model="ir.sequence">
        <field name="name">Inbound Payment Sequence</field>
        <field name="code">send.sequence</field>
        <field name="prefix">CONTRA/WITH/</field>
        <field name="padding">4</field>
    </record>

    <record id="sequence_account_payment_outbound_receive" model="ir.sequence">
        <field name="name">Outbound Payment Sequence</field>
        <field name="code">receive.sequence</field>
        <field name="prefix">CONTRA/DEP/</field>
        <field name="padding">4</field>
    </record>
    <record id="sequence_ledgerpayment_inbound_outbound_receive" model="ir.sequence">
        <field name="name">Ledger Payment Sequence</field>
        <field name="code">ledger.payment.ledger</field>
        <field name="prefix">BR/24-25/</field>
        <field name="padding">4</field>
    </record>
    <record id="sequence_ledgerpayment_bill_inbound_outbound_receive" model="ir.sequence">
        <field name="name">Ledger Payment Bill Wise Adjustments</field>
        <field name="code">ledger.payment.bill</field>
        <field name="prefix">BWA/24-25/</field>
        <field name="padding">4</field>
    </record>
</odoo>