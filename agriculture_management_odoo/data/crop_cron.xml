<odoo>
    <record id="ir_cron_crop_planning" model="ir.cron">
        <field name="name">Check the Dates for Location</field>
        <field name="model_id" ref="model_crop_planning"/>
        <field name="state">code</field>
        <field name="code">model.update_date_for_today()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
