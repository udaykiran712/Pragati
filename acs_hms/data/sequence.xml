<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hms_patient_evaluation" model="ir.sequence">
            <field name="name">ACS Patient Evaluation</field>
            <field name="code">acs.patient.evaluation</field>
            <field name="prefix">EVA</field>
            <field name="padding">3</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hms_patient_treaatment" model="ir.sequence">
            <field name="name">ACS Patient Treatment</field>
            <field name="code">hms.treatment</field>
            <field name="prefix">TREATMENT/ID/</field>
            <field name="padding">3</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hms_appointment" model="ir.sequence">
            <field name="name">ACS Appointment</field>
            <field name="code">hms.appointment</field>
            <field name="prefix">OP</field>
            <field name="padding">5</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hms_prescription_order" model="ir.sequence">
            <field name="name">Prescription Order</field>
            <field name="code">prescription.order</field>
            <field name="prefix">PRO</field>
            <field name="padding">3</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hms_acs_patient_procedure" model="ir.sequence">
            <field name="name">Patient Procedure</field>
            <field name="code">acs.patient.procedure</field>
            <field name="prefix">DP</field>
            <field name="padding">3</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
