<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <!-- Patient Sequences -->
        <record id="seq_patient_sequence" model="ir.sequence">
            <field name="name">Patient Barcode</field>
            <field name="code">patient.barcode</field>
            <field name="prefix">000000</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="acs_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
