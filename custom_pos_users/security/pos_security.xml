<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="pos_config_rule_partner" model="ir.rule">
        <field name="name">POS Config: Restrict to Partner's Assigned Shop</field>
        <field name="model_id" ref="point_of_sale.model_pos_config"/>
