<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data>
       <record id="wbl_product_limit_cart_qty" model="ir.ui.view">
        <field name="name">product.template.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='public_categ_ids']" position="after">
                <field name="min_sale_qty"/>
                <field name="max_sale_qty"/>
                <field name="min_limit_of_product"/>
                <field name="max_limit_of_product"/>
            </xpath>
        </field>
    </record>
   </data>
</odoo>
