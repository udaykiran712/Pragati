<odoo>
    <record id="view_stock_move_line_tree_inherit_on_hand" model="ir.ui.view">
        <field name="name">stock.move.line.tree.inherit.on_hand</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_dest_id']" position="before">
                <field name="on_hand_qty" string="On Hand Quantity" readonly="1"/>
            </xpath>
            <xpath expr="//field[@name='qty_done']" position="before">
                <field name="destination_location_qty" string="On Hand dest Quantity" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
